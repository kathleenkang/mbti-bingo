{
  "name": "react-grid-draw-ui",
  "homepage": "https://ShaneCreedon.github.io/React-grid-draw-ui",
  "version": "1.2.2",
  "description": "A public React NPM library that enables developers to draw grid(s) on top of their web page UI and use the contents within a 2D array data structure.",
  "main": "./dist/src/index.js",
  "scripts": {
    "build": "webpack",
    "build-dev": "./node_modules/.bin/webpack -p --env=development",
    "build-prod": "./node_modules/.bin/webpack -p --env=production",
    "build-prod-demo-gh-page": "./node_modules/.bin/webpack -p --env=production && cp ./index.html dist/src",
    "dev": "./node_modules/.bin/webpack-dev-server --env=development",
    "transpile": "babel src -d dist --copy-files",
    "build-npm-lib": "bash build_for_publish.sh",
    "deploy": "gh-pages -d dist/src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShaneCreedon/React-grid-draw-ui.git"
  },
  "author": {
    "name": "Shane Creedon",
    "url": "https://www.skybreak.app"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ShaneCreedon/React-grid-draw-ui/issues"
  },
  "keywords": [
    "react",
    "grid",
    "draw",
    "canvas",
    "mouse",
    "event",
    "drag",
    "copy",
    "mouse-down",
    "npm",
    "package"
  ],
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/node": "^14.14.2",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.4",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^4.3.0",
    "gh-pages": "^3.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "style-loader": "^1.3.0",
    "ts-loader": "^8.0.7",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "peerDependencies": {
    "react-dom": "^16.13.1",
    "react": "^16.13.1",
    "@types/react-dom": "^16.9.8"
  },
  "dependencies": {}
}
